using System.Text;

namespace com.CIMthetics.CSharpSECSTools.SECSCommUtils
{
	abstract public class SECSHeader
	{
		public UInt32 SystemBytes { get; set; }

		abstract public byte[] EncodeForTransport();

	} // End abstract public class SECSHeader
}
